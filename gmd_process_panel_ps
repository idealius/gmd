${execi 2 sed -n '3,3p;4q' ~/.conky/gmd/.redcpu | cut -d' ' -f 1} $alignr ${execi 2 sed -n '3,3p;4q' ~/.conky/gmd/.redcpu | cut -d' ' -f 2}%
${execi 2 sed -n '4,4p;5q' ~/.conky/gmd/.redcpu | cut -d' ' -f 1} $alignr ${execi 2 sed -n '4,4p;5q' ~/.conky/gmd/.redcpu | cut -d' ' -f 2}%
${execi 2 sed -n '5,5p;6q' ~/.conky/gmd/.redcpu | cut -d' ' -f 1} $alignr ${execi 2 sed -n '5,5p;6q' ~/.conky/gmd/.redcpu | cut -d' ' -f 2}%

